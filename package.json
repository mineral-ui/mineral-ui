{
  "name": "mineral-ui",
  "author": "CA Technologies",
  "description": "Mineral UI component library",
  "version": "0.5.0",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mineral-ui/mineral-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mineral-ui/mineral-ui/issues"
  },
  "homepage": "http://mineral-ui.com",
  "main": "./dist/index.js",
  "module": "./dist/es/index.js",
  "scripts": {
    "build": "npm run clean && ./scripts/build.js",
    "build:analyze": "ANALYZE=true npm run start",
    "build:icons": "./scripts/build-icons.js",
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "eslint": "eslint .",
    "flow": "flow",
    "flow:restart": "npm run flow -- stop; npm run flow",
    "flow-typed": "flow-typed",
    "format": "./scripts/format.js && eslint --fix .",
    "jest": "jest",
    "jest:debug": "node --inspect --inspect-brk node_modules/.bin/jest",
    "lint": "npm run eslint",
    "pack-ls": "cd dist && tar -tvf $(npm pack) && rm *.tgz",
    "postinstall": "npm run build:icons",
    "release": "npm run build && npm test && ./scripts/release.js",
    "release:dist": "./scripts/release.js",
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "test": "npm run lint && npm run flow:restart && npm run jest -- --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "exenv": "1.2.2",
    "immutability-helper": "2.3.1",
    "react-popper": "0.7.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-flow-react-proptypes": "4.1.0",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "colorable": "1.0.5",
    "commitizen": "2.9.6",
    "conventional-changelog-cli": "1.3.2",
    "copy-webpack-plugin": "4.0.1",
    "core-js": "2.5.1",
    "css-loader": "0.28.4",
    "cz-conventional-changelog": "2.0.0",
    "dedent": "0.7.0",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "4.4.1",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.2.1",
    "flow-bin": "0.52.0",
    "flow-copy-source": "1.2.0",
    "flow-typed": "2.1.5",
    "fs-extra": "4.0.1",
    "glamor": "2.20.39",
    "glamorous": "4.8.0",
    "glob": "7.1.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "inquirer": "3.2.3",
    "jest": "20.0.4",
    "jest-glamor-react": "3.1.0",
    "lodash.camelcase": "4.3.0",
    "lodash.flatten": "4.4.0",
    "lodash.upperfirst": "4.3.1",
    "marksy": "5.0.0",
    "material-design-icons": "3.0.1",
    "mustache": "2.3.0",
    "polished": "1.4.1",
    "prepend-file": "1.3.1",
    "prettier": "1.5.3",
    "pretty-bytes": "4.0.2",
    "prop-types": "15.5.10",
    "raf": "3.3.2",
    "raw-loader": "0.5.1",
    "react": "15.6.1",
    "react-docgen": "3.0.0-beta7",
    "react-docgen-loader": "2.9.1",
    "react-dom": "15.6.1",
    "react-live": "1.7.0",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "15.6.2",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "svgo": "0.7.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "validate-commit-msg": "2.12.2",
    "webpack": "3.5.4",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.7.1"
  },
  "peerDependencies": {
    "glamor": "^2.0.0",
    "glamorous": "^4.8.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
