    "react-router-dom": "4.3.1",
    "regenerator-runtime": "0.13.3",
    "rimraf": "2.6.2",
    "style-loader": "1.1.3",
    "svgo": "1.3.2",
    "validate-commit-msg": "2.14.0",
    "webpack": "4.41.6",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.27",
