{
  "name": "mineral-ui",
  "author": "CA Technologies",
  "description": "Mineral UI component library",
  "version": "0.9.0",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mineral-ui/mineral-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mineral-ui/mineral-ui/issues"
  },
  "homepage": "http://mineral-ui.com",
  "main": "./dist/index.js",
  "module": "./dist/es/index.js",
  "scripts": {
    "build": "npm run clean && ./scripts/build.js",
    "build:analyze": "ANALYZE=true npm run start",
    "build:icons": "./scripts/build-icons.js",
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "eslint": "eslint .",
    "flow": "flow",
    "flow:restart": "npm run flow -- stop; npm run flow",
    "flow-typed": "flow-typed",
    "format": "./scripts/format.js && eslint --fix .",
    "jest": "jest",
    "jest:debug": "node --inspect --inspect-brk node_modules/.bin/jest",
    "lint": "npm run eslint",
    "pack-ls": "cd dist && tar -tvf $(npm pack) && rm *.tgz",
    "postinstall": "npm run build:icons",
    "release": "npm run build && npm test && ./scripts/release.js",
    "release:dist": "./scripts/release.js",
    "start": "webpack-dev-server --progress",
    "test": "npm run lint && npm run flow:restart && npm run jest -- --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "exenv": "1.2.2",
    "react-popper": "0.7.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-flow-react-proptypes": "5.2.0",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "colorable": "1.0.5",
    "commitizen": "2.9.6",
    "conventional-changelog-cli": "1.3.4",
    "copy-webpack-plugin": "4.1.0",
    "core-js": "2.5.1",
    "css-loader": "0.28.7",
    "cz-conventional-changelog": "2.0.0",
    "dedent": "0.7.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "enzyme-to-json": "3.1.0",
    "eslint": "4.8.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-flowtype": "2.37.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.56.0",
    "flow-copy-source": "1.2.1",
    "flow-typed": "2.1.5",
    "fs-extra": "4.0.2",
    "glamor": "2.20.40",
    "glamorous": "4.9.7",
    "glob": "7.1.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "inquirer": "3.3.0",
    "jest": "21.2.1",
    "jest-glamor-react": "3.1.1",
    "lodash.camelcase": "4.3.0",
    "lodash.flatten": "4.4.0",
    "lodash.startcase": "4.4.0",
    "lodash.upperfirst": "4.3.1",
    "marksy": "5.0.0",
    "material-design-icons": "3.0.1",
    "mustache": "2.3.0",
    "polished": "1.7.0",
    "prepend-file": "1.3.1",
    "prettier": "1.7.4",
    "pretty-bytes": "4.0.2",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "raw-loader": "0.5.1",
    "react": "16.0.0",
    "react-docgen": "3.0.0-beta7",
    "react-docgen-loader": "2.9.1",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-live": "1.7.1",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.0.0",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "svgo": "0.7.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.9.1"
  },
  "peerDependencies": {
    "glamor": "^2.0.0",
    "glamorous": "^4.8.0",
    "react": "^15.6.1 || ^16.0.0",
    "react-dom": "^15.6.1 || ^16.0.0"
  }
}
